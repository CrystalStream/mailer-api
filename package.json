{
  "name": "generator-api",
  "version": "0.1.0",
  "description": "",
  "private": true,
  "author": "Altamir Consulting, LLC",
  "engines": {
    "node": ">=11.0.0"
  },
  "scripts": {
    "postinstall": "cd cli && npm install",
    "prestart": "if [ ! -d node_modules ]; then npm install; fi",
    "start": "node dist/bin/www",
    "serve": "npm run start",
    "build": "tsc",
    "heroku-postbuild": "npm run build",
    "dev": "nodemon -r ./src/bin/www --exec ts-node -e ts --trace-sync-io",
    "docker:dev": "docker-compose -f docker-compose.development.yml up",
    "docker:test": "docker-compose -f docker-compose.test.yml up",
    "test": "LOG_LEVEL=error npm run test:unit",
    "test:unit": "NODE_ENV=test nyc mocha -r ts-node/register 'src/**/*.spec.ts' --exit",
    "typeorm": "ts-node -r ts-node/register ./node_modules/typeorm/cli.js",
    "oclif": "cd cli && oclif",
    "cli": "./cli/bin/run",
    "cli:meta": "cd cli && npm run"
  },
  "nyc": {
    "include": [
      "src/**/*.ts"
    ],
    "exclude": [
      "src/**/*.spec.ts",
      "src/config/logger/*",
      "**/*.d.ts"
    ],
    "require": [
      "ts-node/register"
    ],
    "extension": [
      ".ts"
    ],
    "reporter": [
      "text",
      "text-summary"
    ],
    "report-dir": "./reports",
    "all": true
  },
  "dependencies": {
    "apollo-server-express": "^2.3.1",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.3",
    "class-validator": "^0.9.1",
    "compression": "^1.7.3",
    "cron": "^1.6.0",
    "dotenv": "^6.2.0",
    "errorhandler": "^1.5.0",
    "express": "^4.16.4",
    "express-validator": "^5.3.1",
    "graphql": "^14.1.1",
    "graphql-tools": "^4.0.3",
    "handlebars": "^4.0.12",
    "helmet": "^3.15.0",
    "jsonwebtoken": "^8.4.0",
    "lodash": "^4.17.11",
    "md5": "^2.2.1",
    "nodemailer": "^5.1.1",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "pg": "^7.8.0",
    "reflect-metadata": "^0.1.12",
    "typeorm": "^0.2.11",
    "validator": "^10.11.0",
    "winston": "^3.1.0"
  },
  "devDependencies": {
    "@altamir/standards-tsconfig": "^1.0.3",
    "@altamir/standards-tslint": "^1.0.4",
    "@types/bcrypt": "^3.0.0",
    "@types/chai": "^4.1.7",
    "@types/chai-http": "^3.0.5",
    "@types/express": "^4.16.0",
    "@types/graphql": "^14.0.5",
    "@types/handlebars": "^4.0.40",
    "@types/jsonwebtoken": "^8.3.0",
    "@types/mkdirp": "^0.5.2",
    "@types/mocha": "^5.2.5",
    "@types/morgan": "^1.7.35",
    "@types/node": "^10.12.18",
    "@types/passport": "^1.0.0",
    "@types/uuid": "^3.4.4",
    "@types/walk": "^2.3.0",
    "@types/validator": "^10.9.0",
    "chai": "^4.2.0",
    "chai-http": "^4.2.1",
    "chai-uuid": "^1.0.6",
    "gulp": "^4.0.0",
    "gulp-rename": "^1.4.0",
    "gulp-template": "^5.0.0",
    "js-yaml": "^3.12.1",
    "minimist": "^1.2.0",
    "mkdirp": "^0.5.1",
    "mocha": "^5.2.0",
    "morgan": "^1.9.1",
    "nodemon": "^1.18.9",
    "nyc": "^13.1.0",
    "oclif": "^1.12.8",
    "ts-node": "^7.0.1",
    "typescript": "^3.2.2",
    "walk": "^2.3.14"
  }
}
